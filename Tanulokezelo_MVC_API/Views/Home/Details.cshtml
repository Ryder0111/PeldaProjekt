@model Tanulokezelo_MVC_API.Models.Student;

<h2 class="alert-info">@Model.Name adatai</h2>
<table class="table table-hover table-bordered">
    <thead>
        <tr>
            <th>Azonosítója</th>
            <th>OM azonosító</th>
            <th>Neve</th>
            <th>Kora</th>
            <th>Átlaga</th>
            <th>Taj száma</th>
        </tr>
    </thead>
    <tbody>
            <tr>
                <td>@Model.Id</td>
                <td>@Model.OMidentifier</td>
                <td>@Model.Name</td>
                <td>@Model.Age</td>
                <td>@Model.GPA</td>
                <td>@Model.TAJnum</td>
            </tr>
    </tbody>
</table>

<a class="btn btn-info mt-3" asp-controller="Home" asp-action="Index">Vissza a főoldalra</a>